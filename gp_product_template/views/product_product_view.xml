<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
       <record id="product_product_tree_view_view_inherit_l10n_mn_mak_product" model="ir.ui.view">
            <field name="name">product.product.tree.inherit.l10n_mn_mak_product</field>
            <field name="model">product.product</field>
            <field name="inherit_id" ref="product.product_product_tree_view"/>
            <field name="arch" type="xml">
                <xpath expr='//field[@name="ean13"]' position="before">
                    <field name="categ_id"/>
                </xpath>
                <xpath expr='//field[@name="name"]' position="before">
                    <field name="product_number"/>
                </xpath>
            </field>
        </record>        
	</data>
</openerp>

